# ---- Cloud Run service ----
PROJECT_ID=
REGION=europe-west1
SERVICE_NAME=open-ui-chat

IMAGE=ghcr.io/danny-avila/open-ui-chat-dev:latest
CPU=2
MEMORY=2Gi
ALLOW_UNAUTHENTICATED=true

# ---- Runtime ----
HOST=0.0.0.0
# Cloud Run injects PORT automatically

# ---- External services ----
MONGO_URI=
MEILI_HOST=

# Optional Meilisearch key (if your app needs it to authenticate to Meili)
MEILI_MASTER_KEY=

# ---- Secrets (leave blank to auto-generate on first deploy) ----
JWT_SECRET=
JWT_REFRESH_SECRET=
CREDS_KEY=
CREDS_IV=

# ---- Open/no-auth flags ----
ALLOW_EMAIL_LOGIN=false
ALLOW_REGISTRATION=false
ALLOW_SOCIAL_LOGIN=false
ALLOW_SOCIAL_REGISTRATION=false

# ---- Provider API keys (optional) ----
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# ---- Networking (optional) ----
VPC_CONNECTOR=
VPC_EGRESS=all-traffic

# ---- Domain mapping (optional) ----
MAP_DOMAIN=false
CUSTOM_DOMAIN=


